{
  "name": "hacktoberfest2018",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEBUG=hacktoberfest2018:* node ./bin/www",
    "dev": "cross-env DEBUG=hacktoberfest2018:* nodemon ./bin/www",
    "format": "prettier-eslint --write \"**/*.js\"",
    "lint": "prettier-eslint -l error --list-different \"**/*.js\"",
    "heroku-postbuild": "cd frontend/ && yarn install --production=false && yarn run build",
    "heroku-prebuild": "yarn config delete proxy && npm config rm https-proxy && npm config rm proxy && yarn install --production=false --network-timeout 1000000"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "engines": {
    "node": "8.11.4",
    "yarn": "1.9.4"
  },
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "@firebase/database-types": "^0.3.2",
    "@octokit/rest": "^15.13.0",
    "axios": "^0.18.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "firebase-admin": "^6.0.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongodb": "^3.1.6",
    "morgan": "~1.9.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.1",
    "nodemon": "^1.18.4",
    "prettier-eslint-cli": "^4.7.1"
  }
}
